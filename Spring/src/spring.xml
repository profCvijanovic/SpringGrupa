<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	
	<!-- <context:property-placeholder location="mojPropertifajl.properties"/> -->
	
	<bean id="matematicar" class="spring.ProfesorMatematike">
		<constructor-arg name="predmet" ref ="mojPredmet" /> <!-- Constructor injection  -->
	</bean>
	
	<bean id="hemicar" class="spring.ProfesorHemije">
		<property name="predmet" ref = "mojPredmet"/> <!-- Setter injection  -->
	</bean>
	
    <bean id = "mojPredmet" class="spring.Predmet">
    </bean>
    
 
    

</beans>